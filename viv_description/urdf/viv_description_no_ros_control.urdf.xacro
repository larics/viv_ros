<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="viv">
    <xacro:property name="M_PI" value="3.1415926535897931" />

    <xacro:arg name="robot_namespace" default="/" />

    <link name="viv_base" >
      <visual>
        <origin xyz="0.0 0 0.1" rpy="0 0 3.1415926535897931" />
        <geometry>
          <mesh filename="package://viv_description/meshes/viv_body.dae" />
        </geometry>
      </visual>

      <!-- collisions -->

      <!--three main cylinders-->
      <collision>
        <origin xyz="0.34 0 0.1" rpy="1.57079 0 0" />
        <geometry>
          <cylinder radius="0.1" length="0.45"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="-0.34 0 0.1" rpy="1.57079 0 0" />
        <geometry>
          <cylinder radius="0.1" length="0.45"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="0 0 0.235" rpy="1.57079 0 0" />
        <geometry>
          <cylinder radius="0.125" length="0.45"/>
        </geometry>
      </collision>
      <!--mounting cylinders-->
      <collision>
        <origin xyz="-0.3 0 0.228" rpy="0 0 0" />
        <geometry>
          <cylinder radius="0.145" length="0.032"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="0.3 0 0.228" rpy="0 0 0" />
        <geometry>
          <cylinder radius="0.145" length="0.032"/>
        </geometry>
      </collision>
      <!--side bars-->
      <collision>
        <origin xyz="0.2 -0.22 0.28" rpy="1.57079 0.349 0" />
        <geometry>
          <box size="0.35 0.1 0.03"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="0.2 0.22 0.28" rpy="1.57079 0.349 0" />
        <geometry>
          <box size="0.35 0.1 0.03"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="-0.2 -0.22 0.28" rpy="1.57079 -0.349 0" />
        <geometry>
          <box size="0.35 0.1 0.03"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="-0.2 0.22 0.28" rpy="1.57079 -0.349 0" />
        <geometry>
          <box size="0.35 0.1 0.03"/>
        </geometry>
      </collision>

      <inertial>
        <origin xyz="0 0 0.2"/>
        <mass value="50"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
                izz="1.0" />
      </inertial>
    </link>

    <!--wheels-->
    <link name="viv_wheel_front_left" >
      <visual>
        <geometry>
          <mesh filename="package://viv_description/meshes/viv_wheel.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.0 0.0 0.0" rpy="1.57079 0 0" />
        <geometry>
          <cylinder radius="0.08" length="0.05"/>
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0"/>
        <mass value="1"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
                izz="1.0" />
      </inertial>
    </link>

