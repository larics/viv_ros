        <mass value="1"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
                izz="1.0" />
      </inertial>
    </link>

     <link name="viv_wheel_rear_left" >
      <visual>
        <geometry>
          <mesh filename="package://viv_description/meshes/viv_wheel.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.0 0.0 0.0" rpy="1.57079 0 0" />
        <geometry>
          <cylinder radius="0.08" length="0.05"/>
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0 0 0"/>
        <mass value="1"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                iyy="1.0" iyz="0.0" 
